<Content_pnx>:
    adaptive_height: True
    orientation: "vertical"
    height: (app.height/12)*4
    
    MDCard:
        size_hint: 1, 1
        md_bg_color:249/255, 184/255, 21/255, 1
        MDBoxLayout:
            padding: dp(10)
            orientation: "horizontal"            
            MDIconButton:
                size_hint: 0.1, 1
                icon: "light_1.png" 
            MDBoxLayout:
                orientation: "vertical"
